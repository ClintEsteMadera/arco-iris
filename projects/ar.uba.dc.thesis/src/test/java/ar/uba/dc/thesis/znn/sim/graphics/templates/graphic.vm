<html>
<head>
	<meta http-equiv="Cache-Control" CONTENT="no-cache"/>
	<meta http-equiv="Pragma" CONTENT="no-cache"/>
	<meta http-equiv="Expires" CONTENT="0"/>
	<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1"/>
    <script language="javascript" type="text/javascript" src="flot/jquery.js"></script>
    <script language="javascript" type="text/javascript" src="flot/jquery.flot.js"></script>
	<meta http-equiv="Refresh" content="1">
</head>
<body class="datos_sistema" leftmargin="0" topmargin="0" marginheight="0" marginwidth="10">

	<h1>Experience Response Time Average</h1>

	<div id="experRespTimeDiv" style="width:800;height:400px;"></div>

	<h1>Server Cost Sum</h1>

	<div id="costDiv" style="width:800px;height:400px;"></div>

</body>

<script type="text/javascript">
	var experRespTimePoints = new Array(1000);
	var costPoints = new Array(1000);
	$experRespTimePoints
	$costPoints

</script>

<script id="source" language="javascript" type="text/javascript">
$(function () {

    $plot($("#experRespTimeDiv"), [
        {
            data: experRespTimePoints,
            points: { show: true },
            lines: { show: true }
        }
    ]);

    $plot($("#costDiv"), [
        {
            data: costPoints,
            points: { show: true },
            lines: { show: true }
        }
    ]);
});
</script>

</html>