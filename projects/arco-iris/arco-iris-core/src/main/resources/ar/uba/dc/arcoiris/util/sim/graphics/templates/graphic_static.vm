<html>
<head>
	<meta http-equiv="Cache-Control" CONTENT="no-cache"/>
	<meta http-equiv="Pragma" CONTENT="no-cache"/>
	<meta http-equiv="Expires" CONTENT="0"/>
	<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1"/>
	<script language="javascript" type="text/javascript" src="../flot/jquery.min.js"></script>
	<script language="javascript" type="text/javascript" src="../flot/jquery.flot.min.js"></script>
	<script language="javascript" type="text/javascript" src="../flot/jquery.flot.threshold.min.js"></script>
</head>
<body class="datos_sistema" leftmargin="0" topmargin="0" marginheight="0" marginwidth="10">

	<h3>Experience Response Time</h3>

	<div id="experRespTimeDiv" style="width:1200;height:350px;"></div>

	<h3>Server Cost</h3>

	<div id="costDiv" style="width:1200;height:350px;"></div>

</body>

<script type="text/javascript">
	var experRespTimePoints = new Array(1000);
	var costPoints = new Array(1000);
	$experRespTimePoints
	$costPoints

</script>

<script id="source" language="javascript" type="text/javascript">
$(function () {

    $plot($("#experRespTimeDiv"), [
        {
            data: experRespTimePoints,
            color: "rgb(200, 20, 30)",
            #if ($experRespTimeThreshold)
	            threshold: { below: $experRespTimeThreshold, color: "rgb(30, 180, 20)" },
			#else
            	color: "rgb(30, 180, 20)",
	        #end
			points: { show: false },
			lines: { show: true }
        }
    ]);

    $plot($("#costDiv"), [
        {
            data: costPoints,
            color: "rgb(200, 20, 30)",
            #if ($costThreshold)
	            threshold: { below: $costThreshold, color: "rgb(30, 180, 20)" },
			#else
            	color: "rgb(30, 180, 20)",
	        #end
			points: { show: false },
			lines: { show: true }
        }
    ]);

});
</script>

</html>