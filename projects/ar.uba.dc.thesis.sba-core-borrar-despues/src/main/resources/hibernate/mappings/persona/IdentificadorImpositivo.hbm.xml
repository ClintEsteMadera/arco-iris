<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" package="sba.common.persona.identificacion">

	<class name="IdentificadorImpositivo" table="IDENTIF_IMPOSITIVO">

		<id name="id">
			<generator class="sequence" />
		</id>

		<version name="persistenceVersion" column="VERSION_NUMBER" />

		<property name="numero" />

		<property name="tipoIdentificadorImpositivo" column="TIPO">
			<type name="sba.common.persistence.EnumUserType">
				<param name="enumClassName">
					sba.common.persona.identificacion.TipoIdentificadorImpositivo
				</param>
			</type>
		</property>

	</class>

</hibernate-mapping>
