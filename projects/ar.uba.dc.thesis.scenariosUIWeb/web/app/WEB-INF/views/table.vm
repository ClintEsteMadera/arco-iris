<html>
	<head>
		#parse("common_header.vm")
		<link rel="stylesheet" href="#springUrl('/css/jmesa.css')" type="text/css"/>
		<title>${title}</title>
		<script type="text/javascript" src="#springUrl('/js/jmesa/jquery-1.3.2.min.js')"></script>
		<script type="text/javascript" src="#springUrl('/js/jmesa/jquery.jmesa.js')"></script>
		<script type="text/javascript" src="#springUrl('/js/jmesa/jmesa.js')"></script>
	</head>
	<body onload="userNotification()">
		#title(${title})
		<form name="enumTableForm" action="">
			${tableHtml}
		</form>
		<script type="text/javascript">
			function onInvokeAction(id) {
				setExportToLimit(id, '');
				createHiddenInputFieldsForLimitAndSubmit(id);
			}
		</script>
	</body>
</html>